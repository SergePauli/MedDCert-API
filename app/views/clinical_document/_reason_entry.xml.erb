<entry>
	<observation classCode="OBS" moodCode="EVN">
	  <code code="4050" codeSystem="1.2.643.5.1.13.13.99.2.166" codeSystemVersion="1.73" displayName="Прочие важные состояния, способствовавшие смерти" codeSystemName="Кодируемые поля CDA документов"/>
	  <effectiveTime <% if reason_entry.is_effective_time? %>value="<%= reason_entry.fmt_effective_time(@certificate.death_datetime) %>"<% else %>nullFlavor="<%= reason_entry && reason_entry.nullFlavor("effectiveTime") || "NA" %>"<% end %>/>
